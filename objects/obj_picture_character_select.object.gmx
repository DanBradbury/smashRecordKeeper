            <kind>1</kind>
            <string>// INFO: create a way to break out
if(keyboard_check_pressed(vk_escape)) {
  instance_destroy();
}

// INFO: check the list of characters
//      - simply drawing the size of the sprites w/ some logic for multiple rows
//      - logic for hover using selected_item + mouse_x/y
for(var i=0;i&lt;character_count;i++) {
  sprite_x = 0+((i)*43);
  sprite_y = 0;

  if(i&gt;=66) {
    sprite_x = 0+((i-66)*43);
    sprite_y = 75;
  } else if(i&gt;=44) {
    sprite_x = 0+((i-44)*43);
    sprite_y = 50;
  } else if(i&gt;=22) {
    sprite_x = 0+((i-22)*43);
    sprite_y = 25;
  }

  if(mouse_x &gt;= sprite_x &amp;&amp; mouse_x &lt;= 43+sprite_x &amp;&amp; mouse_y &gt;= sprite_y &amp;&amp; mouse_y &lt;= sprite_y+24) {
    selected_item[i] = 1;
    if(mouse_check_button_pressed(mb_left)) {
      var a = add_character_to_list(ds_list_find_value(character_list, i));
      instance_destroy();
    }
  } else {
    selected_item[i] = 0;
  }
}

</string>
